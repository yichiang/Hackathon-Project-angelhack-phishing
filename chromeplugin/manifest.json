{
  "manifest_version": 2,
  "name": "Project SWORDPHISH Demo",
  "description": "This extension demos the SWORDPHISH content guard",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_title": "SWORDPHISH Content Guard",
    "default_popup": "popup.html"
  }
}
